<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Qoute Generator</title>
</head>
<body>

	<section class="qoute__container">
		<div>
			<h3 class="header">Random Qoute Generator</h3>
			<hr>
		<% allData.forEach(function(qoutes){ %>
			<p class="qoute"><%=qoutes.qoute %></p>
			<i class="qoute"> - <%=qoutes.author%></i> <br>
		<% }) %>
				
		</div>
	</section>


</body>
</html>